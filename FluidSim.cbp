<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="FluidSim" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug Windows">
				<Option output="bin/Debug/FluidSim" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="--demo2" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add option="-lmingw32 -lSDL2main -lSDL2" />
				</Linker>
			</Target>
			<Target title="Debug Linux">
				<Option output="FluidSim" prefix_auto="1" extension_auto="1" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Option parameters="-l 400" />
				<Linker>
					<Add option="-lSDL2main -lSDL2" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/FluidSim" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add directory="C:/Win-builds/include" />
		</Compiler>
		<Linker>
			<Add directory="C:/Win-builds/lib" />
		</Linker>
		<Unit filename="fluidsim.h" />
		<Unit filename="gui.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
